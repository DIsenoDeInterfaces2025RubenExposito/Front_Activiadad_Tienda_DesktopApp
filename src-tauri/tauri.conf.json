{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AppTiendaMueblesRuben",
  "version": "0.1.0",
  "identifier": "com.ruben.tiendamuebles.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173/",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "title": "TiendaMueblesRuben",
        "width": 375,
        "height": 667,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}